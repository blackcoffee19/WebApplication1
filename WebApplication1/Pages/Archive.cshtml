@page
@model ArchiveMode 
@{

}
<div class="row">
    <div class="col">
        <h2>Mission Completed:</h2>
        <hr>
        @for(int i = 0; i < Model.orderTask.Count; i++){
            <div>
                <h3><a asp-page="/Archive" asp-route-index=@i>@Model.orderTask[i].Title</a></h3>
            </div>
        }
    </div>
    <div class="col">
        <h3 class="archive-post-title">@Model.Displayed.Title</h3>
        <p>@Model.Displayed.Mission</p>
    </div>
</div>